# 서버 설정
PORT=3001
NODE_ENV=production

# # 텔레그램 봇 설정 (선택사항)
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
# TELEGRAM_CHAT_ID=your_chat_id_here

# # 카카오톡 알림 설정 (선택사항)
# KAKAO_API_KEY=your_kakao_api_key_here

# 데이터베이스 설정 (선택사항 - 현재는 메모리/파일 기반)
